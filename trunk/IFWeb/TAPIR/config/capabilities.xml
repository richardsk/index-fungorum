	<capabilities>
		<operations>
			<ping/>
			<metadata/>
			<capabilities/>
			<inventory>
				<anyConcepts/>
			</inventory>
			<search>
				<outputModels>
					<anyOutputModels>
						<responseStructure>
							<basicSchemaLanguage/>
						</responseStructure>
					</anyOutputModels>
				</outputModels>
			</search>
		</operations>
		<requests>
			<encoding>
				<kvp/>
				<xml/>
			</encoding>
			<globalParameters>
				<logOnly>[LOG_ONLY]</logOnly>
			</globalParameters>
			<filter>
				<encoding>
					<expression>
						<concept/>
						<literal/>
						<parameter/>
						<variable/>
					</expression>
					<booleanOperators>
						<logical>
							<not/>
							<and/>
							<or/>
						</logical>
						<comparative>
							<equals caseSensitive="[CASE_SENSITIVE_EQUALS]"/>
							<greaterThan/>
							<greaterThanOrEquals/>
							<lessThan/>
							<lessThanOrEquals/>
							<in/>
							<isNull/>
							<like caseSensitive="[CASE_SENSITIVE_LIKE]"/>
						</comparative>
					</booleanOperators>
				</encoding>
			</filter>
		</requests>
		[MAPPING_XML]
		<variables>
			<environment>
				<date/>
				<timestamp/>
				<dataSourceName/>
				<accessPoint/>
				<lastUpdated/>
				<dateCreated/>
				<metadataLanguage/>
				<dataSourceLanguage/>
				<dataSourceDescription/>
				<rights/>
			</environment>
		</variables>
		<settings>
			<maxElementRepetitions>[MAX_ELEMENT_REPETITIONS]</maxElementRepetitions>
			<maxElementLevels>[MAX_ELEMENT_LEVELS]</maxElementLevels>
		</settings>
	</capabilities>
	