	<capabilities>
		<operations>
			<ping/>
			<metadata/>
			<capabilities/>
			<inventory>
				<anyConcepts/>
			</inventory>
			<search>
				<outputModels>
					<anyOutputModels>
						<responseStructure>
							<basicSchemaLanguage/>
						</responseStructure>
					</anyOutputModels>
				</outputModels>
			</search>
		</operations>
		<requests>
			<encoding>
				<kvp/>
				<xml/>
			</encoding>
			<globalParameters>
				<logOnly>accepted</logOnly>
			</globalParameters>
			<filter>
				<encoding>
					<expression>
						<concept/>
						<literal/>
						<parameter/>
						<variable/>
					</expression>
					<booleanOperators>
						<logical>
							<not/>
							<and/>
							<or/>
						</logical>
						<comparative>
							<equals caseSensitive="false"/>
							<greaterThan/>
							<greaterThanOrEquals/>
							<lessThan/>
							<lessThanOrEquals/>
							<in/>
							<isNull/>
							<like caseSensitive="false"/>
						</comparative>
					</booleanOperators>
				</encoding>
			</filter>
		</requests>
			<concepts>
		<conceptNameServers/>
		<schema namespace="http://rs.tdwg.org/dwc/dwcore/" location="http://rs.tdwg.org/dwc/tdwg_dw_core.xsd">
			<mappedConcept searchable="true">http://rs.tdwg.org/dwc/dwcore/GlobalUniqueIdentifier</mappedConcept>
			<mappedConcept searchable="true">http://rs.tdwg.org/dwc/dwcore/DateLastModified</mappedConcept>
			<mappedConcept searchable="true">http://rs.tdwg.org/dwc/dwcore/BasisOfRecord</mappedConcept>
			<mappedConcept searchable="true">http://rs.tdwg.org/dwc/dwcore/InstitutionCode</mappedConcept>
			<mappedConcept searchable="true">http://rs.tdwg.org/dwc/dwcore/CollectionCode</mappedConcept>
			<mappedConcept searchable="true">http://rs.tdwg.org/dwc/dwcore/CatalogNumber</mappedConcept>
			<mappedConcept searchable="true">http://rs.tdwg.org/dwc/dwcore/ScientificName</mappedConcept>
		</schema>
	</concepts>

		<variables>
			<environment>
				<date/>
				<timestamp/>
				<dataSourceName/>
				<accessPoint/>
				<lastUpdated/>
				<dateCreated/>
				<metadataLanguage/>
				<dataSourceLanguage/>
				<dataSourceDescription/>
				<rights/>
			</environment>
		</variables>
		<settings>
			<maxElementRepetitions>200</maxElementRepetitions>
			<maxElementLevels>20</maxElementLevels>
		</settings>
	</capabilities>
	